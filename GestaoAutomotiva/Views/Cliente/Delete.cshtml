@model GestaoAutomotiva.Models.Cliente

<h2 class="text-light">Excluir Cliente</h2>

<p>Tem certeza que deseja excluir o cliente <strong>@Model.Nome</strong>?</p>

<form method="post">
    <button type="submit" class="btn btn-danger">Excluir</button>
    <a asp-action="Index" class="btn btn-secondary">Cancelar</a>
</form>
